                <div class="text-center col-md-12 form">
                    <h1>B.Tech Registration Form</h1>
                    <h1 class="text-danger">DO REGISTER</h1>
                </div>
            </div><!--row end-->
            <div class="row">
                <div class="col-md-6">
                    <div class="carousel slide mt-4" data-bs-ride="carousel" id="a">
                        <div class="carousel-indicators">
                          <button type="button"data-bs-target="#a"data-bs-slide-to="0"class="active"></button>
                          <button type="button" data-bs-target="#a" data-bs-slide-to="1"></button>
                          <button type="button" data-bs-target="#a" data-bs-slide-to="2"></button>
                        </div><!--carousel-indicators-->
                  
                        <div class="carousel-inner">
                            <div class="carousel-item active imageheight" data-bs-interval="3000">
                              <img class="w-100" src="https://mits-admission-portal.onrender.com/assets/images/registration_form1.jpg" alt="">
                            </div>
                            <div class="carousel-item imageheight" data-bs-interval="2000">
                                <img class="w-100 " src="https://mits-admission-portal.onrender.com/assets/images/registration_form2.jpg" alt="">
                              </div>
                              <div class="carousel-item imageheight" data-bs-interval="2000">
                                <img class="w-100" src="https://mits-admission-portal.onrender.com/assets/images/registration_form3.jpg" alt="">
                              </div>
                        </div><!--carousel-inner end-->
                        <button class="carousel-control-prev" type="button" data-bs-target="#a" data-bs-slide="prev">
                            <span class="carousel-control-prev-icon"></span>
                        </button>
                        <button class="carousel-control-next" type="button" data-bs-target="#a" data-bs-slide="next">
                            <span class="carousel-control-next-icon"></span>
                        </button>
                     </div><!--carousel slide end-->
                </div>
                <div class="col-md-6">
                    <div class="fluid-container">
                        <div class="row bg">
                          <div class="col-md-12 ">
                            <div class="container loginblock register">
                              <form>
                                <div class="mb-3 mt-4">
                                  <label class="mb-2" style="opacity: 0.6">Username:</label>
                                  <input type="text" name="" placeholder="Name" class="form-control h"required>
                                </div>
                                <div class="mb-3">
                                    <label class="mb-2" style="opacity: 0.6">Email:</label>
                                  <input type="email" name="" placeholder="Email" class="form-control h" required>
                                </div>
                                <div class="mb-3">
                                    <label class="mb-2" style="opacity: 0.6">Mobile No.</label>
                                  <input type="number" name="" placeholder="Enter Mobile Number" class="form-control h" required>
                                </div>
                                <div class="mb-3">
                                    <label class="mb-2" style="opacity: 0.6">Date:</label>
                                  <input type="date" name="" class="form-control h"required>
                                </div>
                                <div class="mb-3">
                                    <label class="mb-2" style="opacity: 0.6">Gender:</label>
                                    <br>
                                 <input type="radio" name="" value="male">&nbsp; Male
                                 &nbsp;
                                 <input type="radio" name="" value="female">&nbsp; Female
                                </div>
                                <div class="mb-3">
                                    <label class="mb-2" style="opacity: 0.6">Address:</label>
                                  <textarea name="" class="form-control h"required>
                                </textarea>
                                </div>
                                <p style="opacity: 0.6;">Profile Picture</p>
                                <div class="mb-3">
                                  <input type="file"name="image"class="form-control"style="margin-top: -15px"required>
                                </div>
                                <div class="mb-3">
                                    <label class="mb-2" style="opacity: 0.6">Select College:</label>
                                  <select name="" class="form-control h"required>
                                    <option>Select</option>
                                    <option>Mits</option>
                                    <option>ITM</option>
                                    <option>Rjit</option>
                                    <option>NITM</option>
                                </select>
                                </div>
                                <div class="mb-3">
                                    <label class="mb-2" style="opacity: 0.6">Course:</label>
                                  <select name="" class="form-control h"required>
                                    <option>Select</option>
                                    <option>B.Tech</option>
                                </select>
                                </div>
                                <div class="mb-3">
                                    <label class="mb-2" style="opacity: 0.6">Branch:</label>
                                  <select name="" class="form-control h"required>
                                    <option>Select</option>
                                    <option>CSS</option>
                                    <option>IT</option>
                                    <option>EC</option>
                                    <option>ET</option>
                                    <option>ME</option>
                                    <option>EE</option>
                                </select>
                                </div>
                                
                                <button class="form-control">Register</button>
                                <h6>HAVE A ACCOUNT? <a href="login.html">LOGIN NOW</a></h6>
                              </form>
                            </div>
                          </div>
                        </div>
                      </div>
                </div>
            </div>
        </div>
    </div>

